default:	
	@cd src/ && $(MAKE)
	@cp  output/lzo_compress .
	@echo "make successful ! "
	@echo "Executable created in output directory"

lzo:	
	@cd src/ && $(MAKE)
	@cp  output/lzo_compress .
	@echo "make successful ! "
	@echo "Executable created in output directory"
clean:
	@echo "Cleaning output ...."
	@rm -f output/*
	@echo "Cleaning obj directory ...."
	@rm -f obj/*.o
	@echo "Cleaning Complete."